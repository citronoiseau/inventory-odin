<div class="navigation">
  <div class="breeds-container">
    <div>Check out our breeds:</div>
    <div class="breeds-list">
      <% if (!breeds || breeds.length === 0) { %>
      <p>No breeds found.</p>
      <% } else { %> <% breeds.forEach((breed) => { %>
      <div class="breed-type">
        <a href="/filter/breed/<%= breed.id %>"><%= breed.name %></a>
      </div>
      <% }); %> <% } %>
    </div>
  </div>
  <a href="/breed/add" class="link-button">Add new breed</a>
</div>
